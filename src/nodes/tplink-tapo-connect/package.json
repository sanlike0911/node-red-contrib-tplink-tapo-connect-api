{
  "name": "tplink-tapo-connect",
  "version": "1.0.0",
  "description": "TypeScript library for TP-Link Tapo smart device control",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/index.ts",
    "example:plug": "ts-node examples/plug-example.ts",
    "example:bulb": "ts-node examples/bulb-example.ts",
    "example:wrapper": "ts-node examples/wrapper-example.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "postinstall": "npm run build"
  },
  "keywords": [
    "tplink",
    "tapo",
    "smart-home",
    "iot",
    "typescript",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.12.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.0",
    "local-devices": "^4.0.0",
    "node-red": "^4.1.0",
    "uuid": "^11.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}